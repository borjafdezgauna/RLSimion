<RLSimion FileVersion="1.0.0.0">
<RLSimion>
	<Log>
		<Num-functions-logged>10</Num-functions-logged>
		<Log-Freq>0.25</Log-Freq>
		<Log-eval-episodes>true</Log-eval-episodes>
		<Log-training-episodes>false</Log-training-episodes>
		<Log-functions>true</Log-functions>
	</Log>
	<World>
		<Num-Integration-Steps>4</Num-Integration-Steps>
		<Delta-T>0.01</Delta-T>
		<Dynamic-Model>
			<Model>
		<Rain-car>
		</Rain-car>
		</Model>
		</Dynamic-Model>
	</World>
	<Experiment>
		<Random-Seed>1</Random-Seed>
		<Num-Episodes>100</Num-Episodes>
		<Eval-Freq>10</Eval-Freq>
		<Progress-Update-Freq>1.0</Progress-Update-Freq>
		<Episode-Length>120.0</Episode-Length>
	</Experiment>
	<SimGod>
		<Target-Function-Update-Freq>100</Target-Function-Update-Freq>
		<Gamma>0.9</Gamma>
		<Freeze-Target-Function>false</Freeze-Target-Function>
		<Use-Importance-Weights>false</Use-Importance-Weights>
		<State-Feature-Map>
			<Input-State>
				<Input-State>position</Input-State>
			</Input-State>
			<Num-Features-Per-Dimension>10</Num-Features-Per-Dimension>
			<Feature-Mapper>
				<Type>
		<Discrete-Grid>
		</Discrete-Grid>
		</Type>
			</Feature-Mapper>
		</State-Feature-Map>
		<Action-Feature-Map>
			<Input-Action>
				<Input-Action>acceleration</Input-Action>
			</Input-Action>
			<Num-Features-Per-Dimension>10</Num-Features-Per-Dimension>
			<Feature-Mapper>
				<Type>
		<Discrete-Grid>
		</Discrete-Grid>
		</Type>
			</Feature-Mapper>
		</Action-Feature-Map>
		<Experience-Replay>
			<Buffer-Size>1000</Buffer-Size>
			<Update-Batch-Size>10</Update-Batch-Size>
		</Experience-Replay>
		<Simion>
			<Type>
		<Q-Learning>
			<Policy>
				<Policy>
		<Epsilon-Greedy>
			<Epsilon>
				<Schedule>
		<Simple-Linear-Decay>
			<Initial-Value>0.5</Initial-Value>
			<End-Value>0.0</End-Value>
		</Simple-Linear-Decay>
		</Schedule>
			</Epsilon>
		</Epsilon-Greedy>
		</Policy>
			</Policy>
			<Q-Function>
				<Init-Value>0.0</Init-Value>
			</Q-Function>
			<Alpha>
				<Schedule>
		<Simple-Linear-Decay>
			<Initial-Value>0.1</Initial-Value>
			<End-Value>0.0</End-Value>
		</Simple-Linear-Decay>
		</Schedule>
			</Alpha>
		</Q-Learning>
		</Type>
		</Simion>
	</SimGod>
</RLSimion>
</RLSimion>
